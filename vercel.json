{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" },
        { "key": "Permissions-Policy", "value": "xr-spatial-tracking=(self)" }
      ]
    }
  ],
  "functions": {
    "api/*.js": {
      "regions": ["cdg1", "fra1", "dub1"],
      "maxDuration": 10,
      "memory": 1024
    }
  }
}
